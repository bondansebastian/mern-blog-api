{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES6"],
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist",
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES5",
  },
  "include": ["./api/*.ts"],
}
