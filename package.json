{
  "name": "mern-blog-api",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "prod": "npm run clean && rollup --config",
    "dev": "npm run clean && babel ./src --extensions \".ts\" --out-dir dist",
    "start": "npm run dev && node ./dist/index.js",
    "clean": "rimraf dist",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "ext": "ts",
    "watch": [
      "src/*",
      "dist/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.3",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.3",
    "rollup-plugin-dotenv": "^0.5.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
